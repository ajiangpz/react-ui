{
  "name": "react-mobile-ui",
  "version": "1.0.0",
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "types": "./dist/index.d.ts"
    },
    "./button": {
      "import": "./dist/es/components/Button/index.js",
      "types": "./dist/components/Button/index.d.ts"
    },
    "./tabbar": {
      "import": "./dist/es/components/TabBar/index.js",
      "types": "./dist/components/TabBar/index.d.ts"
    },
    "./popup": {
      "import": "./dist/es/components/Popup/index.js",
      "types": "./dist/components/Popup/index.d.ts"
    },
    "./actionsheet": {
      "import": "./dist/es/components/ActionSheet/index.js",
      "types": "./dist/components/ActionSheet/index.d.ts"
    },
    "./styles.css": "./dist/es/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "rimraf dist && tsc && vite build && node scripts/copy-package.js",
    "build:watch": "rollup -c -w",
    "build:es": "rollup -c ./config/rollup.config.es.mjs",
    "build:umd": "rollup -c ./config/rollup.config.umd.mjs",
    "build:iife": "rollup -c ./config/rollup.config.iife.mjs",
    "build:storybook": "storybook build",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore --fix",
    "test": "vitest",
    "prepare": "husky"
  },
  "keywords": [
    "react",
    "mobile",
    "ui",
    "components",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "description": "A lightweight, beautiful and touch-friendly React UI library for mobile web applications",
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.17",
    "crypto-js": "^4.2.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.47",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@use-gesture/react": "^10.3.1",
    "ant-design": "^1.0.0",
    "antd": "^5.25.4",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.344.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.2.1",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.0"
  },
  "volta": {
    "node": "20.16.0"
  }
}
