{
  "name": "@tendaui/monorepo",
  "version": "1.0.0",
  "description": "TendaUI React Component Library Monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "prebuild": "rimraf packages/tendaui-react/es",
    "build": "npm run prebuild && rollup -c rollup.config.mjs && npm run build:tsc-es",
    "build:components": "npm run prebuild && rollup -c rollup.config.mjs && npm run build:tsc-es",
    "build:tsc-es": "tsc -p tsconfig.build.json --outDir packages/tendaui-react/es",
    "build:icons": "pnpm -w --filter @tendaui/icons build",
    "build:utils": "pnpm -w --filter @tendaui/utils build",
    "dev": "node scripts/dev.js",
    "dev:storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "node scripts/clean.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "test": "pnpm -r test",
    "type-check": "pnpm -r type-check",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "install:all": "pnpm install",
    "update:deps": "pnpm update -r",
    "check:workspace": "pnpm list --depth=0",
    "outdated": "pnpm outdated -r"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "keywords": [
    "react",
    "ui",
    "component",
    "monorepo",
    "tendaui"
  ],
  "author": "ajiangz",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "9.38.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-url": "^8.0.2",
    "@rollup/pluginutils": "^5.3.0",
    "@storybook/addon-docs": "9.1.8",
    "@storybook/react-vite": "9.1.8",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "5.14.2",
    "@vitejs/plugin-react": "^5.0.3",
    "camelcase": "^6.2.0",
    "clsx": "^2.1.1",
    "commitlint": "20.0.0",
    "fast-glob": "^3.3.3",
    "hoist-non-react-statics": "^3.3.2",
    "lucide-react": "^0.544.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-static-import": "^1.0.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-pluginutils": "^2.8.2",
    "sass": "^1.93.2",
    "storybook": "9.1.8",
    "svgo": "^4.0.0",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.14",
    "tdesign-react": "^1.15.1",
    "tendaui-react": "^1.0.1",
    "tendaui-react-icons": "1.1.3",
    "tinycolor2": "^1.6.0"
  },
  "dependencies": {
    "@babel/runtime": "~7.26.7",
    "@popperjs/core": "~2.11.2",
    "@types/sortablejs": "^1.10.7",
    "@types/validator": "^13.1.3",
    "classnames": "~2.5.1",
    "dayjs": "1.11.10",
    "hoist-non-react-statics": "~3.3.2",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.544.0",
    "mitt": "^3.0.0",
    "raf": "~3.4.1",
    "react-fast-compare": "^3.2.2",
    "react-is": "^18.2.0",
    "react-transition-group": "~4.4.1",
    "sortablejs": "^1.15.0",
    "tslib": "~2.3.1",
    "validator": "~13.15.0",
    "vite": "^7.1.7"
  },
  "volta": {
    "node": "22.20.0"
  }
}
